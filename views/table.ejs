<table>
    <thead>
        <tr>
            <th>姓名</th>
            <th>年龄</th>
            <th>性别</th>
        </tr>
    </thead>
    <tbody>
        <%for(var i=0;i<items.length;i++){%>
            <tr>
                <td><%=items[i].name%></td>
                <td><%=items[i].age%></td>
                <td><%=items[i].sex%></td>
            </tr>
        <%}%>
    </tbody>
</table>